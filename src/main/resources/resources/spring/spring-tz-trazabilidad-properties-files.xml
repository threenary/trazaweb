<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

	<!--+++++++++++++++++++++++++++++++++++++++++++-->
	<!--| ARCHIVO DE PROPIEDADES TIPO PLACEHOLDER |-->
	<!--+++++++++++++++++++++++++++++++++++++++++++-->
	<bean id="propertiesPlaceHolderFileWorkflow" class="java.util.ArrayList">
		<constructor-arg>
			<list>
				<value>classpath:/resources/properties/workflow.properties</value>
				<value>classpath:/resources/properties/sntm.properties</value>
			</list>
		</constructor-arg>
	</bean>

</beans>